{
  "kind": "collectionType",
  "collectionName": "rooms",
  "info": {
    "singularName": "room",
    "pluralName": "rooms",
    "displayName": "Room",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "nbBeds": {
      "type": "integer"
    },
    "nbPeople": {
      "type": "integer"
    },
    "isAvailable": {
      "type": "boolean",
      "default": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "default": "slug"
    },
    "details": {
      "type": "customField",
      "options": {
        "preset": "default"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    },
    "price": {
      "type": "string",
      "required": true
    },
    "currency": {
      "type": "string"
    },
    "priority": {
      "type": "integer",
      "default": 1,
      "required": true
    },
    "subtitle": {
      "type": "string"
    }
  }
